{
  "name": "potato-swaps",
  "version": "0.0.1",
  "description": "A little database of potato alternatives for those who don't like potatoes",
  "main": "dist/server/index.js",
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf coverage/",
    "compile-server": "npx tsc -p tsconfig.server.json",
    "build": "npm run clean && npm run compile-server",
    "start": "node dist/server/index.js",
    "dev": "nodemon src/server/index.ts",
    "lint": "npx eslint .",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielle95/potato-swaps.git"
  },
  "keywords": [
    "node",
    "expressjs",
    "react",
    "typescript",
    "potato",
    "alternative"
  ],
  "author": "gabrielle95",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gabrielle95/potato-swaps/issues"
  },
  "homepage": "https://github.com/gabrielle95/potato-swaps#readme",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0"
  }
}
